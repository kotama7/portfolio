{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/selectFunction", "function": "selectFunction" },
      { "source": "/autoReply", "function": "autoReply" }
    ]
  }
}
